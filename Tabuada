#include <iostream>
using namespace std;
int main()
{
    int tabuada;
    cout << "Qual tabuada vocÃª que ver?" << endl;
    cin >> tabuada;
    int x[10];
    for(int i = 0; i<=10; i++){
        x[i] = i * tabuada;
        cout << tabuada << " X " << i << " = " << x[i] << endl;
    }
    cout << "Press enter to continue ...";
    cin.get();
    return 0;
}
